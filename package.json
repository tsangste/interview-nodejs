{
  "name": "rest-api-example-starter",
  "version": "1.0.0",
  "description": "A simple REST API using Express and body-parser",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "format": "prettier --write \"{src,apps,libs,test}/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "build": "npm run clean && tsc",
    "start": "nodemon --watch src -e ts src/index.ts",
    "start:prod": "node dist/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^16.18.2",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "keywords": [
    "rest",
    "express",
    "api",
    "body-parser"
  ]
}
